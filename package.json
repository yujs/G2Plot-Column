    "lib:esm": "tsc -p tsconfig.json --target ES5 --module ESNext --outDir esm",
    "prepublishOnly": "npm run lib:cjs && npm run lib:esm && npm run lib:umd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yujs/G2Plot-Column.git"
  },
  "author": "yujs",
